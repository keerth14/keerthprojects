import random
lowercase = "abcdefghijklmnopqrstuvwxyz"
uppercase = "ABCDEFGHIJKLMNOPQRSTUVWXYZ"
number = "1234567890"
symbols = "!@#$%^&*()_+=<>?,./\"\";:\'\\|~`"
all = lowercase + uppercase + number + symbols
length = int(input())
password = "".join(random.sample(all,length))
print("The password is ",password)
